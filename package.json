{
	"name": "connects-you-user",
	"packageManager": "yarn@3.2.3",
	"description": "",
	"scripts": {
		"build": "tsc -p tsconfig.release.json",
		"postinstall": "husky install && tsc -p tsconfig.release.json",
		"prestart": "tsc -p tsconfig.release.json",
		"start": "node ./build/index.js",
		"dev": "nodemon ./src/index.ts -e ts,json",
		"startLocalServer": "ngrok http 1000",
		"kill": "sudo kill -9 `sudo lsof -t -i:1000`",
		"testMultiDocumentTransactionInMongoDB": "tsc -p tsconfig.release.json && node ./build/src/scripts/testMultiDocumentTransactionInMongoDB.js",
		"testScript": "tsc -p tsconfig.release.json && node ./build/src/scripts/test.js",
		"lint": "eslint ./src --ext .ts,.js --fix"
	},
	"author": "Adarsh Mishra",
	"license": "ISC",
	"dependencies": {
		"@adarsh-mishra/node-utils": "https://github.com/AdarshMishraji/node-utils",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.0.2",
		"express-http-context": "^1.2.4",
		"express-rate-limit": "^6.6.0",
		"google-auth-library": "^8.5.2",
		"tsutils": "^3.21.0"
	},
	"devDependencies": {
		"@types/compression": "^1.7.2",
		"@types/express": "^4.17.14",
		"@types/express-rate-limit": "^6.0.0",
		"@types/jsonwebtoken": "^8.5.9",
		"@types/node": "^18.7.19",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"eslint": "^8.24.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-sonarjs": "^0.15.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.8.3"
	},
	"version": "0.0.1"
}
